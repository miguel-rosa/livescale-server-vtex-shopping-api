{
  "memory": 256,
  "ttl": 10,
  "timeout": 2,
  "minReplicas": 2,
  "maxReplicas": 4,
  "workers": 1,
  "routes": {
    "status": {
      "path": "/_v/status/:code",
      "public": true
    },
    "baskets": {
      "path": "/_v/baskets",
      "public": true
    },
    "basketsItems": {
      "path": "/_v/baskets/:basketId/items",
      "public": true
    },
    "basketsItemsChange": {
      "path": "/_v/baskets/:basketId/items/:itemId",
      "public": true
    }
  }
}
