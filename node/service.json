{
  "memory": 256,
  "ttl": 10,
  "timeout": 2,
  "minReplicas": 2,
  "maxReplicas": 4,
  "workers": 1,
  "routes": {
    "basketsCreate": {
      "path": "/_v/baskets",
      "public": true
    },
    "basketsUpdate": {
      "path": "/_v/baskets/:basketId/items",
      "public": true
    },
    "basketsList": {
      "path": "/_v/baskets/:basketId",
      "public": true
    },
    "basketsUpdateItem": {
      "path": "/_v/baskets/:basketId/items/:itemId",
      "public": true
    },
    "catalogsList": {
      "path": "/_v/catalogs",
      "public": true
    },
    "categoriesList": {
      "path": "/_v/catalogs/:catalogId/categories",
      "public": true
    },
    "productsList": {
      "path": "/_v/catalogs/:catalogId/categories/:categoryId/products",
      "public": true
    },
    "salesChannelsList": {
      "path": "/_v/sales-channels",
      "public": true
    }
  }
}
